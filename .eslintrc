{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "defaultParams": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "import/no-extraneous-dependencies": 0,
    "arrow-body-style": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0
  }
}
